@model IEnumerable<_420_476.Projet.Session.Models.User>

@{
    ViewBag.Title = "GererProfils";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

<h2>GererProfils</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Statut_disponible)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Password)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Membre.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Role.Label)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Statut_disponible)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Membre.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Role.Label)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) @*|*@
            @*@Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })*@
        </td>
    </tr>
}

</table>
